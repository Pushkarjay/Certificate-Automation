{
  "name": "certificate-automation",
  "version": "1.0.0",
  "description": "Certificate Automation System - Monorepo",
  "private": true,
  "scripts": {
    "build": "cd Frontend/React && npm install && npm run build",
    "start": "cd backend && npm start",
    "dev": "npm run dev:backend & npm run dev:frontend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd Frontend/React && npm start",
    "install:all": "cd backend && npm install && cd ../Frontend/React && npm install",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd Frontend/React && npm install",
    "postinstall": "npm run install:all && npm run build",
    "clean": "rm -rf backend/node_modules Frontend/React/node_modules",
    "clean:win": "rmdir /s /q backend\\node_modules Frontend\\React\\node_modules"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "certificate",
    "automation",
    "nodejs",
    "react",
    "fullstack",
    "monorepo"
  ],
  "author": "SURE Trust",
  "license": "MIT"
}
